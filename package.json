    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^6.30.1",
    "react-router-dom": "^6.30.1",
    "tailwindcss": "^3.4.17",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
